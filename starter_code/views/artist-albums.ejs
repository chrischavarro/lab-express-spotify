<h1>Artist Albums</h1>
<ul>
  <% albums.forEach((album) => { %>
    <% if (album.images[0] !== undefined ) {%>
      <img src=" <%= album.images[0].url %>" width="300px">
    <% } %>
      <h2><%= album.name %></h2>
      <a href="/album/tracks/<%=album.id %>"><button type="button">View Tracks</button></a>
    <ul></ul>
  <% })  %>
</ul>
